<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" default-lazy-init="false">
	
	<!-- ****************** -->
	<!-- configurations -->
	<!-- ****************** -->
	<bean id="configuration" class="it.quix.academy.qborrow.Configuration">
		<property name="rowPerPage" value="10" />
		<property name="urlApplication" value="localhost:8082/qborrow" />
		<property name="urlWsdl" value="http://localhost:8082/qborrow/managerws?wsdl" />
		<property name="baseYearForLastUpdateTimeColumn" value="1899" />
		<property name="baseMonthForLastUpdateTimeColumn" value="12" />
		<property name="baseDayForLastUpdateTimeColumn" value="30" />
		<!-- Graphic Version -->
		<property name="graphicVersion" value="2"/>
	</bean>	
</beans>