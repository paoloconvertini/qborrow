	<tag>
		<name>pager</name>
		<tag-class>it.quix.commons.taglib.PagerTag</tag-class>
		<body-content>JSP</body-content>
		<description>Tag per visualizzare un paginatore</description>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Nome dell'attributo che contiene il paginatore
			</description>
		</attribute>
		<attribute>
			<name>scope</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Scopo da cui recuperare il pager: request o session
				(String)
			</description>
		</attribute>
		<attribute>
			<name>property</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Se scope=session e il pager è contenuto all'interno di
				un bean, il nome della proprietà che contiene il Pager
			</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Funzione javascript da richiamare prima del cambio di
				pagina
			</description>
		</attribute>
		<attribute>
			<name>tableStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Classe che specifica lo stile della tabella creata dal
				Pager
			</description>
		</attribute>
		<attribute>
			<name>cellStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Classe che specifica lo stile di una cella della tabella
				creata dal pager
			</description>
		</attribute>
		<attribute>
			<name>linkStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Classe che specifica lo stile dei link per il cambio di
				pagina generati dal pager
			</description>
		</attribute>
		<attribute>
			<name>selectedStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Classe che contiene lo stile del numero della pagina
				corrente
			</description>
		</attribute>
	</tag>
