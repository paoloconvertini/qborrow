	<tag>
		<name>imagePopup</name>
		<tag-class>it.quix.framework.web.taglib.popup.ImagePopupTag</tag-class>
		<body-content>empty</body-content>
		<display-name>Quix Image Popup Tag</display-name>
		<description>Tag per disegnare un'immagine che apre una popup</description>
		<attribute>
			<name>image</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Immagine da visualizzare, senza imagePath.</description>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Id dell'immagine</description>
		</attribute>
		<attribute>
			<name>imageheight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Altezza dell'immagine.</description>
		</attribute>
		<attribute>
			<name>imagewidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Larghezza dell'immagine.</description>
		</attribute>
		<attribute>
			<name>imagestyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Allineamento verticale dell'immagine.</description>
		</attribute>
		<attribute>
			<name>altimage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Testo alternativo dell'immagine.</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Url da chiamare, o funzione javascript che restituisce l'url da chiamare.</description>
		</attribute>
		<attribute>
			<name>urlisfunction</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Se l'url e' una funzione.</description>
		</attribute>
		<attribute>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Titolo della popup aperta.</description>
		</attribute>
		<attribute>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Larghezza della popup.</description>
		</attribute>
		<attribute>
			<name>height</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Altezza dell'immagine.</description>
		</attribute>
		<attribute>
			<name>absolute</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Se le dimensioni sono assolute o relative.</description>
		</attribute>
		<attribute>
			<name>modal</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>EXCLUSIVE o MODAL.</description>
		</attribute>
		<attribute>
			<name>draggable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>-DRAGGABLE o DRAGGABLE.</description>
		</attribute>
		<attribute>
			<name>relx</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>RELX o MIDX.</description>
		</attribute>
		<attribute>
			<name>valrelx</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>valore di RELX o MIDX.</description>
		</attribute>
		<attribute>
			<name>rely</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>RELY o MIDY.</description>
		</attribute>
		<attribute>
			<name>valrely</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>valore di RELY o MIDY.</description>
		</attribute>
		<attribute>
			<name>closeclik</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>CLOSECLICK o NOCLOSE o MOUSEOFF.</description>
		</attribute>
		<attribute>
			<name>imgcolose</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Percorso dell'immagine di chiusura.</description>
		</attribute>
		<attribute>
			<name>altimgclose</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Testo alternativo dell'immagine di chiusura.</description>
		</attribute>
		<attribute>
			<name>iframename</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Nome dell'iframe generato.</description>
		</attribute>
		<attribute>
			<name>returnfunction</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>funzione di ritorno da chiamare alla chiusura della popup.</description>
		</attribute>
		<attribute>
			<name>scroll</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Gestione dello scroll.</description>
		</attribute>
		<attribute>
			<name>submitform</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Impostare a true per effettuare il submit del form della pagina padre. Default 'false'</description>
		</attribute>
		<attribute>
			<name>formexpression</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Espressione Javascript per recuperare il form di cui effettuare il submit. Default 'document.forms[0]'</description>
		</attribute>
		<attribute>
			<name>framework</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>True per puntare alla cartella delle immagini del framework, false per quella dell'applicazione. Default: false.</description>
		</attribute>
	</tag>
